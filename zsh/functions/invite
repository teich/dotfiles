function invite-cmd {
  heroku run bin/invite-token --to=$1 -a canvas-api-production 2> /dev/null
}

# if a second argument run that # of times.
if [ -n "${2}" ]; then
  for i in {1..$2}; do
    invite-cmd $1 | grep Token | cut -d ' ' -f 2
  done
else
  invite-cmd $1 | grep Token | cut -d ' ' -f 2
fi
